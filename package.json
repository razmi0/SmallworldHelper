{
  "name": "smallworld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --port 42069",
    "build": "./svg/run.sh && tsc && vite build",
    "icons:build": "./svg/run.sh",
